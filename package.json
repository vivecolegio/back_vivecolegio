{
  "name": "backendserver-vivecolegios",
  "version": "1.0.0",
  "description": "",
  "main": "./app/index.ts",
  "scripts": {
    "dev": "DEBUG=app:* nodemon ./app/index",
    "start:dev": "NODE_ENV=production ts-node ./app/index.ts",
    "test": "mocha --exit",
    "cover": "nyc npm run test",
    "report": "nyc report --reporter=html && open coverage/index.html",
    "build": "tsc",
    "dev:server": "nodemon ./app/servers/index.ts",
    "development": "concurrently -k npm:development:*",
    "development:server": "ts-node ./app/servers/index.ts",
    "development:gateway": "wait-on tcp:4001 && ts-node ./app/index.dev",
    "start": "concurrently -k npm:server:*",
    "server:server": "ts-node ./app/servers/index.ts",
    "server:gateway": "wait-on tcp:4001 && ts-node ./app/index",
    "sonar-scanner": "sonar-scanner",
    "build:docker": "docker build -f Dockerfile -t lidertecnologicovivecolegios/backendvivecolegios .",
    "push:docker": "docker push lidertecnologicovivecolegios/backendvivecolegios",
    "build:docker-offline": "docker build -f Dockerfile -t lidertecnologicovivecolegios/backendvivecolegios-offline .",
    "push:docker-offline": "docker push lidertecnologicovivecolegios/backendvivecolegios-offline"
  },
